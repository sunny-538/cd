%{
#include <stdio.h>
%}

%%
[ \t\r\n]+                  ;
"//".*                      { printf("Comment: %s\n", yytext); }
"/*"([^*]|\*+[^*/])*\*+"/"  { printf("Multiline Comment: %s\n", yytext); }
[a-zA-Z_][a-zA-Z0-9_]*      { printf("Identifier: %s\n", yytext); }
[0-9]+                      { printf("Constant: %s\n", yytext); }
"=="|"!="|"<="|">="         { printf("Operator: %s\n", yytext); }
[=+\-*/<>]                  { printf("Operator: %s\n", yytext); }
.                           { printf("Unknown: %s\n", yytext); }
%%

int main() { yylex(); }
int yywrap() { return 1; }
